Public Class Form1

    Private Sub Button1_Click(sender As System.Object, e As System.EventArgs) Handles Button1.Click

        Dim score As Integer
        InputBox("Please Enter Your Score Here", "English")

        If score > 50 Then
            TextBox1.Text = "Well Done You Got An A"
        Else
            TextBox1.Text = "Better Luck Next Time But You Did Get A C"
        End If

    End Sub
End Class
